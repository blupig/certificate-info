{
  "manifest_version": 2,

  "name": "Cert Info",
  "description": "This extension shows certificate information of the pages you visit.",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },

  "permissions": [
    "tabs",
    "https://api.blupig.net/*"
  ]
}
